pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'Running Build'
        echo 'Build finished'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Performing Deployment'
        echo 'Deployment finished'
      }
    }
    stage('Test') {
      steps {
        echo 'Testing Started'
        echo 'Testing complete'
      }
    }
  }
}